{
  "root": true,
  "extends": ["@nuxt", "plugin:prettier/recommended", "prettier"],
  "plugins": ["simple-import-sort"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"

  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
